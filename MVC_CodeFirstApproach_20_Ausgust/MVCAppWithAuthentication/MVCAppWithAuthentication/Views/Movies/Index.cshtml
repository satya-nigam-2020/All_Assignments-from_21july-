@model IEnumerable<MVCAppWithAuthentication.Models.Movies>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>All Movies</h2>

<table class="table tab-content table-bordered table-responsive">
    <tr>
        <td>Movie Id</td>

        <td>Movie Name</td>

        <td>Movie Type</td>
    </tr>
    @foreach (var m in Model)
    {
    <tr>
        <td>@m.Id</td>

        <td>@m.Name</td>

        <td>@m.MovieType</td>
    </tr>
    }
</table>

